{
  "name": "olaf-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.4.0",
    "formik": "^2.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "react-share": "^5.2.2",
    "react-slick": "^0.31.0",
    "web-vitals": "^2.1.4",
    "yup": "^1.7.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "react-scripts test --watchAll=false --coverage --ci",
    "eject": "react-scripts eject",
    "test:e2e": "playwright test",
    "test:functional": "playwright test tests/functional/",
    "test:system": "playwright test tests/system/",
    "test:smoke": "playwright test tests/smoke/",
    "test:e2e:headed": "playwright test --headed",
    "test:report": "playwright test --reporter=html",
    "test:dev": "playwright test --headed --debug",
    "test:performance": "playwright test tests/system/performance.spec.js",
    "test:load": "playwright test tests/e2e/user-journey.spec.js --project=chromium",
    "show-report": "playwright show-report",
    "show-trace": "playwright show-trace"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(axios)/)"
    ],
    "moduleNameMapper": {
      "^axios$": "<rootDir>/__mocks__/axios.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@playwright/test": "^1.56.0",
    "babel-jest": "^30.0.5",
    "identity-obj-proxy": "^3.0.0",
    "webpack-cli": "^6.0.1"
  }
}
